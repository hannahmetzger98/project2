<p id="notice"><%= notice %></p>

<h1>Fitness Classes</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @fitness_classes.each do |fitness_class| %>
      <tr>
        <td><%= link_to 'Show', fitness_class %></td>
        <td><%= link_to 'Edit', edit_fitness_class_path(fitness_class) %></td>
        <td><%= link_to 'Destroy', fitness_class, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Fitness Class', new_fitness_class_path %>
